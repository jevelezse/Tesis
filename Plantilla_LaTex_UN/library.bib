isbn = {1544-6115 (Electronic)\r1544-6115 (Linking)},
issn = {15446115},
journal = {Statistical Applications in Genetics and Molecular Biology},
keywords = {De novo mutations,Mutation rates,Pedigree,Short-read data,Trio model},
