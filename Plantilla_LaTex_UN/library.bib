issn = {19326203},
journal = {PLoS ONE},
number = {2},
pages = {1--22},
pmid = {26840129},
title = {{MutAid: Sanger and NGS based integrated pipeline for mutation identification, validation and annotation in human molecular genetics}},
volume = {11},
year = {2016}
}
